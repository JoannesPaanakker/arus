<%= simple_form_for(@booking) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :name %>
    <input type="text" data-provide='datepicker' name="start_time">Start Date:</inout>
    <input type="text" data-provide='datepicker' name="end_time">End Date:</input>
  </div>

  <div class="form-actions">
    <%= f.button :submit , class: "mybutton tekst" %>
  </div>
<% end %>